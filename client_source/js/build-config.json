{
  "paths": {
    "angular"               : "../lib/angular/angular",
    "angular-bootstrap"     : "../lib/angular-bootstrap/ui-bootstrap",
    "angular-bootstrap-tpls": "../lib/angular-bootstrap/ui-bootstrap-tpls",
    "angular-cookies"       : "../lib/angular-cookies/angular-cookies",
    "angular-resource"      : "../lib/angular-resource/angular-resource",
    "angular-ui-router"     : "../lib/angular-ui-router/release/angular-ui-router",
    "angular-ui-select2"    : "../lib/angular-ui-select2/src/select2",
    "angular-ui-utils-route": "../lib/angular-ui-utils/modules/route/route",
    "async"                 : "../lib/requirejs-plugins/src/async",
    "domReady"              : "../lib/requirejs-domready/domReady",
    "jquery"                : "../lib/jquery/jquery",
    "underscore"            : "../lib/underscore/underscore",
    "text"                  : "../lib/requirejs-text/text",
    "select2"               : "../lib/select2/select2",
    "twbootstrap"           : "../lib/bootstrap-assets/js/bootstrap",
    "fuelux"                : "../lib/fuelux/fuelux"
  },

  "shim": {
    "twbootstrap": {
      "deps": ["jquery"]
    },
    "angular": {
      "exports": "angular",
      "deps": ["jquery", "underscore"]
    },
    "angular-bootstrap": {
      "deps": ["angular", "twbootstrap"]
    },
    "angular-bootstrap-tpls": {
      "deps": ["angular"]
    },
    "angular-ui-router": {
      "deps": ["angular"]
    },
    "angular-ui-utils-route": {
      "deps": ["angular"]
    },
    "angular-ui-select2": {
      "deps": ["angular", "select2"]
    },
    "select2": {
      "deps": ["jquery"]
    },
    "angular-cookies": {
      "deps": ["angular"]
    },
    "angular-mocks": {
      "deps": ["angular"]
    },
    "angular-resource": {
      "deps": ["angular"]
    },
    "fuelux": {
      "deps": ["jquery"]
    }
  },

  "wrap": true,
  "name": "main",
  "optimize": "none",
  "baseUrl": "client_source/js",
  "mainConfigFile": "client_source/js/main.js",
  "out": "public/js/main-src.js"
}
